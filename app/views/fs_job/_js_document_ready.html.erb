
// Bind 'action butttons'
$("#save_fs_button").bind('click', function(e) {
	$(this).html("Saving...");
	$(this).attr("disabled", "disabled");
	ajax_it("save_job_fs_profile");
});

<% if params[:action] == "edit_job_profile" %>
	setTimeout(repeat_functions, 15000, {functionRef: ajax_it, repeatTimes: -1, repeatInterval: 15000}, "search_job_seekers");
<% end %>
