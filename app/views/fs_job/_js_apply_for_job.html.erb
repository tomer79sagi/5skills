
<script type="text/javascript">

var g_editing = false;

function attach_field_binders(field_id) {
	
	// Call common functions
	attach_common_field_binders(field_id);
	
	// --- Bind all fields (if no 'field_id' is provided)
	
	if (!field_id) {
		attach_field_binders("btn_send_application");
		attach_field_binders("btn_cancel_application");
		
		return;
	}
	
	
	// --- Bind the field
	
	switch (field_id) {
			
		case "btn_send_application":
			$("[id='btn_find_jobs']").bind('click', function(e) {
				$("[id='btn_find_jobs']").html("Please wait...");
				$("[id='btn_find_jobs']").attr("disabled", "disabled");
				
				var ajax_data = {
					fs_profile_data: {
						id: $("#fs_profile").attr("entity_id")
					}
				};
				
				// ajax_it("save_job_seeker_fs_profile", ajax_data);
				doRedirect("home");
			});	
			break;
			
		case "btn_cancel_application":
		
			$("[id='btn_find_jobs']").bind('click', function(e) {
				$("[id='btn_find_jobs']").html("Please wait...");
				$("[id='btn_find_jobs']").attr("disabled", "disabled");
				
				var ajax_data = {
					fs_profile_data: {
						id: $("#fs_profile").attr("entity_id")
					}
				};
				
				// ajax_it("save_job_seeker_fs_profile", ajax_data);
				doRedirect("home");
			});	
			break;
						
	}
			
}

</script>