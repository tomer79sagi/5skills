<% @rendering_start = Time.now %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
       "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
  <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
  
  <%= yield :head %>
  
  <%= stylesheet_link_tag 'phase_1/main' %>
  <%= stylesheet_link_tag 'phase_1/messages' %>
  
  <%#= calendar_date_select_includes "red"%>
  
  <%= javascript_include_tag "jquery-1.5.1.min", "application" %>

</head>
<body>

<div class="container">

<%= render :partial => '/main/header_1' %>

<div style="width:100%;text-align:left;padding:1em 0em 0em 0em;">
