
<script type="text/javascript">

var g_editing = false;

function attach_field_binders(field_id) {
	
	// Call common functions
	attach_common_field_binders(field_id);
	
	// --- Bind all fields (if no 'field_id' is provided)
	
	if (!field_id) {
		attach_field_binders("btn_save_fs_profile");
		
		return;
	}
	
	
	// --- Bind the field
	
	switch (field_id) {
			
		case "btn_save_fs_profile":
		
			// Bind 'action butttons'
			$("#save_fs_button").bind('click', function(e) {
				$(this).html("Saving...");
				$(this).attr("disabled", "disabled");
				ajax_it("save_job_seeker_fs_profile");
			});
			break;
						
	}
			
}

</script>